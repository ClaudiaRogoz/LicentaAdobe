                        "height" : "$height"
                    }
                },
                "text.paragraphs" : "$PARAGRAPH 1 state.title rules.style.font.size",
                "text.rawText" : "$state.title",
                "meta.ux.rangedStyles" : "$COPY rules.style.font subtags.meta.ux.rangedStyles",
                "order" : ["transform", "name", "style.fill.color.value", "style.font", "text.frame", "text.rawText", "meta.ux.rangedStyles", "text.paragraphs"]
            },
            "toRemove" : ["style.fill.pattern"]
        }
    },
    "subtags" : {
        
        "transform" : {
            "a":1,
            "b":0,
            "c":0,
            "d":1,
            "tx":"$x",
            "ty":"$y"
        },
        "style" : {
            "fill": {
                "type":"solid",
                "color": {
                    "mode":"RGB",
                    "value":{
                        "r":"$red",
                        "g":"$green",
                        "b":"$blue",
                        "default" : {
                            "r" : 255,
                            "g" : 255,
                            "b" : 255
                        }
                    }
                    
                },
                "pattern" : {
                    "width" : "$width",
                    "height" : "$height",
                    "meta" : {
                        "ux" : {
                            "scaleBehavior":"cover",
                            "uid":"$rand",
                            "hrefLastModifiedDate":1465114273
                        
                        }
                    },
                    "href" : "$href"
                }
            },
            "font":{
                "postscriptName":"$name",
                "family":"$family",
                "style":"Regular",
