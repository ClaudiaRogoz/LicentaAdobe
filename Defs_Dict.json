    "content" : {
        "dependency" : "$document.scenes.scene",
        "view" : {
            "rules" : {
                "type" : "artboard",
                "style.fill.color.value" : {
                    "$objects.viewController.view.color" : {
                        "r" : "$red",
                        "g" : "$green",
                        "b" : "$blue"
                    }
                },
                "children" : {
                    "$objects.viewController.view.subviews" : {}
                }
                
            }
        }
        
    },
    "children" : {
        "text" : {
            
            "rules" : {
                "type":"text",
                "transform" : {
                    "$rect" : {
                        "tx" : "$x",
                        "ty" : "$y"
                    }
                },
                "style.fill.color.value" : {
                    "$color" : {
                        "r" : "$red",
                        "g" : "$green",
                        "b" : "$blue"
                    }
                },
                "style.font" : {
                    "$fontDescription" : {
                        "postscriptName":"$name",
                        "family":"$family",
                        "style":"Regular",
                        "size":"$pointSize"
                    }
                },
                "text.frame" : {
                    "$rect" : {
                        "width" : "$width",
                        "height" : "$height"
                    }
                },
                "text.paragraphs" : "$PARAGRAPH 1 text rules.style.font.size",
                "text.rawText" : "$text",
                "meta.ux.rangedStyles" : "$COPY rules.style.font subtags.meta.ux.rangedStyles",
                "order" : ["transform", "name", "style.fill.color.value", "style.font", "text.frame", "text.rawText", "meta.ux.rangedStyles", "text.paragraphs"]
                
                
            },
            "toRemove" : ["style.fill.pattern"]
            
        },
        "shape" : {
            "rules" : {
                "type":"shape",
                "transform" : {
                    "$rect" : {
                        "tx" : "$x",
                        "ty" : "$y"
                    }
                    
                },
                "style.fill.color.value" : {
                    "$color.$key=backgroundColor" : {
                        "r" : "$red",
                        "g" : "$green",
                        "b" : "$blue"
                    }
                    
                },
                "shape" : {
                    "$rect" : {
                        "type" : "rect",
