                        "width" : "$width",
                        "height" : "$height"
                    }
                    
                }
            },
            "toRemove" : ["style.font", "style.fill.color"]
        
        },
        "buttonText" : {
            "rules" : {
                "type":"text",
                "transform" : {
                    "$rect" : {
                        "tx" : "$x",
                        "ty" : "$y"
                    }
                },
                "style.fill.color.value" : {
                    "$state.color" : {
                        "r" : "$red",
                        "g" : "$green",
                        "b" : "$blue"
                    }
                },
                "style.font" : {
                    "$fontDescription" : {
                        "postscriptName":"$name",
                        "family":"$family",
                        "style":"Regular",
                        "size":"$pointSize"
                    }
                },
                "text.frame" : {
                    "$rect" : {
