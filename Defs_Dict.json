            },
            "toRemove" : ["style.font", "style.fill.pattern"]
            
            
        },
        "image" : {
            "rules" : {
                "type" : "shape",
                "transform" : {
                    "$rect" : {
                        "a":1,
                        "b":0,
                        "c":0,
                        "d":1,
                        "tx":"$x",
                        "ty":"$y"
                    }
                },
                "style.fill.type" : "pattern",
                "style.fill.pattern" : {
                    "$rect" : {
                        "width" : "$width",
                        "height" : "$height"
                    },
                    "$" : {
