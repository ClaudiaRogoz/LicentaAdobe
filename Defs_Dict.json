                "shape" : {
                    "$rect" : {
                        "type" : "rect",
                        "x" : 0,
                        "y" : 0,
                        "width" : "$width",
                        "height" : "$height"
                    }
                
                }
            },
            "toRemove" : ["style.font", "style.fill.color"]
        },
        "buttonImage" : {
            "rules" : {
                "type" : "shape",
