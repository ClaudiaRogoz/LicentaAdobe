{
    
    "artboard"  :   "view",
    "view"      :   "artboard",
    "text"      :   {
        "text.frame.type.positioned" : "label",
        "text.frame.type.area"  : "$textArea"
    },
    "label"     :   "text",
    "shape"     :   {
        "shape.type.rect" : {
            "style.fill.type.pattern" : "imageView",
            "style.fill.type.solid"   : "$frame"
        },
        "shape.type.path" : {
            "style.fill.type.solid" : "$imageView",
            "style.fill.type.none" : "$imageView",
            "style.fill.type.pattern" : "$imageViewPath"
        },
        "shape.type.line" : {
            "style.fill.type.none" : "$lineView"
        }
    },
    "group"     :   "group",
    "$frame"    :   "label",
    "$imageView"    :   "imageView",
    "$lineView"     :   "imageView",
    "$imageViewPath" : "imageView",
    "$imageState" : "state",
    "$labelState" : "state",
    "$textArea"   : "label"
    
}